print("Hello World")
local Player = game:GetService("Players").LocalPlayer
local RS = game:GetService("ReplicatedStorage")
local PlayerGui = Player.PlayerGui
local function CreateGui ()
	if PlayerGui:FindFirstChild("GrowGui") == nil then
	local screengui = Instance.new("ScreenGui")
	screengui.Parent = PlayerGui
	screengui.Enabled = true 
	screengui.Name = "GrowGui"

	
	local MainFrame = Instance.new("Frame")
	MainFrame.Parent = screengui
	MainFrame.Name = "MainFrame"
	MainFrame.Visible = true
	MainFrame.Size = UDim2.new(0.25,0,0.25,0)
	MainFrame.Position = UDim2.new(0.375,0,0.375,0)
	MainFrame.BackgroundColor3 = Color3.fromRGB(98, 186, 79)
	
	local UiDragDetect = Instance.new("UIDragDetector")
	UiDragDetect.Parent = MainFrame
	
	local UiStroke = Instance.new("UIStroke")
	UiStroke.Parent = MainFrame
	UiStroke.Thickness = 3
	
	local CloseButton = Instance.new("TextButton")
	CloseButton.Parent = MainFrame
	CloseButton.BackgroundTransparency = 1
	CloseButton.Name = "Close"
	CloseButton.Text = "Close"
	CloseButton.Font = Enum.Font.GothamBold  local CloneUiStroke = UiStroke:Clone() CloneUiStroke.Parent = CloseButton
	CloseButton.TextScaled = true
	CloseButton.Size = UDim2.new(0.25,0,0.25,0)
	CloseButton.Position = UDim2.new(0.75,0,0,0)

	local TeleportButton = Instance.new("TextButton")
	TeleportButton.Parent = MainFrame
	TeleportButton.BackgroundTransparency = 1
	TeleportButton.Text = "Teleport"
	TeleportButton.Name = "Teleport"
	TeleportButton.TextScaled = true
	TeleportButton.Font = Enum.Font.GothamBold  local CloneUiStroke = UiStroke:Clone() CloneUiStroke.Parent = CloseButton
	TeleportButton.Position = UDim2.new(0,0,0.37,0)
	TeleportButton.Size = UDim2.new(0.463,0,0.25,0)
	
	TeleportButton.Activated:Connect(function()
		for i,Egg in pairs(workspace:WaitForChild("NPCS"):FindFirstChild("Pet Stand"):FindFirstChild("EggLocations"):GetChildren()) do
			if Egg:IsA("Model") then
				print(Egg.Name)
			end
		end
	end)
	else
	PlayerGui:FindFirstChild("GrowGui"):Destroy()
	end
end

CreateGui()
